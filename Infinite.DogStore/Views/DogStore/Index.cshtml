@model List<Infinite.DogStore.Models.Dogs>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<div class="row">
    <div class="col-md-4">
        <table class="table table-hover">
            <tr>
                <th>
                    Id
                </th>
                <th>
                    Dog Name
                </th>
                <th>
                    Description
                </th>
               
                <th>Action</th>
            </tr>
            @foreach (var prd in Model)
            {
                <tr>
                    <td>@prd.Id</td>
                    <td>@prd.DogName</td>
                   
                </tr>
            }
        </table>
    </div>
</div>
